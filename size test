//square handle is 17mm 
difference(){
            translate([50,80,0]){
                cylinder(5,9.8,9.8);
            }
            translate([50,80,-.5]){
                cylinder(6,8.5,8.5,$fn=100);
            }
        }
translate([64,65,1.5]){
    cube([2,7,2]);
}
//cylinder handle is 18mm
difference(){
            translate([80,80,0]){
                cylinder(5,10.3,10.3);
            }
            translate([80,80,-.5]){
                cylinder(6,9,9,$fn=100);
            }
        }
translate([80,97,2.5]){
    rotate([90,90,0]){
        cylinder(7,1.5,1.5,$fn=50);
    }
}
//triangle handle is 17.5mm 
