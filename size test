            translate([65,55,0]){
                cylinder(5,10.15,10.15);
            }
            translate([65,55,-.5]){
                cylinder(6,8.75,8.75,$fn=100);
            }
        }
translate([50,96,2.5]){
    rotate([90,90,0]){
        cylinder(7,1.5,1.5,$fn=3);
    }
}
